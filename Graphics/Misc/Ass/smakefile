all:		3dgame:test/hiredgunscd32/game/Misc.gfx \
		3dgame:test/hiredgunscd32/game/Items.gfx

3dgame:test/hiredgunscd32/game/Items.gfx:	itemscd32.s \
						itemscd32.script \
						/ilbm/itemscd32.ilbm
						ilbm2raw -b6cm -@itemscd32.script /ilbm/itemscd32.ilbm ram:output.bin
						genam itemscd32.s
						strip 3dgame:test/hiredgunscd32/game/Items.gfx 32 4

3dgame:test/hiredgunscd32/game/Misc.gfx:	miscgfxcd32.s \
						/raw/digitscd32.bin \
						/raw/light1.bin \
						/raw/light2.bin \
						/raw/light3.bin \
						/raw/expl1.bin \
						/raw/expl2.bin \
						/raw/expl3.bin \
						/raw/expl4.bin \
						/raw/planet.bin
						genam miscgfxcd32.s
						strip 3dgame:test/hiredgunscd32/game/Misc.gfx 32 4

/raw/planet.bin:	/ilbm/planet.ilbm \
			planet.s
			ilbm2raw -cb1f0,0,96,42 /ilbm/planet.ilbm ram:output.bin
			genam planet.s
			strip /raw/planet.bin 32 4

/raw/expl1.bin:		/ilbm/expl1_64.ilbm \
			expl1_64.s
			ilbm2raw -b124mcf0,0,80,80 /ilbm/expl1_64.ilbm ram:temp1
			ilbm2raw -b124mcf0,80,48,48 /ilbm/expl1_64.ilbm ram:temp2
			ilbm2raw -b124mcf0,128,32,32 /ilbm/expl1_64.ilbm ram:temp3
			ilbm2raw -b124mcf0,160,32,22 /ilbm/expl1_64.ilbm ram:temp4
			ilbm2raw -b124mcf80,0,144,84 /ilbm/expl1_64.ilbm ram:temp5
			join ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 as ram:output.bin
			genam expl1_64.s
			strip /raw/expl1.bin 32 4

/raw/expl2.bin:		/ilbm/expl2_64.ilbm \
			expl2_64.s
