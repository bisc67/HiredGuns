/raw/digitscd32.bin:	digitscd32.s \
			/ilbm/digits.ilbm
			rx digits.rexx
			genam digitscd32.s
			strip /raw/digitscd32.bin 32 4

/raw/light1.bin:        light1.s \
			/ilbm/light1.ilbm
			copy /ilbm/light1.ilbm ram:ilbm
			copy work:c/ilbm2raw ram:
			ram:ilbm2raw -cf80,0,32,25 ram:ilbm ram:temp1
			ram:ilbm2raw -cf112,0,32,25 ram:ilbm ram:temp2
			ram:ilbm2raw -cf144,0,32,25 ram:ilbm ram:temp3
			ram:ilbm2raw -cf176,0,32,25 ram:ilbm ram:temp4
			ram:ilbm2raw -cf208,0,32,25 ram:ilbm ram:temp5
			ram:ilbm2raw -cf64,44,48,31 ram:ilbm ram:temp6
			ram:ilbm2raw -cf112,44,48,31 ram:ilbm ram:temp7
			ram:ilbm2raw -cf160,44,32,31 ram:ilbm ram:temp8
			join ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 ram:temp7 ram:temp8 as ram:output.bin
			ram:ilbm2raw -cf192,44,48,31 ram:ilbm ram:temp1
			ram:ilbm2raw -cf240,44,48,31 ram:ilbm ram:temp2
			ram:ilbm2raw -cf48,123,32,26 ram:ilbm ram:temp3
			ram:ilbm2raw -cf80,123,64,26 ram:ilbm ram:temp4
			ram:ilbm2raw -cf144,123,48,26 ram:ilbm ram:temp5
			ram:ilbm2raw -cf192,123,64,26 ram:ilbm ram:temp6
			ram:ilbm2raw -cf256,123,32,26 ram:ilbm ram:temp7
			ram:ilbm2raw -cf80,208,48,18 ram:ilbm ram:temp8
			join ram:output.bin ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 ram:temp7 ram:temp8 as ram:temp_output
			delete ram:output.bin
			rename ram:temp_output as ram:output.bin
			ram:ilbm2raw -cf128,208,80,18 ram:ilbm ram:temp1
			ram:ilbm2raw -cf208,208,48,18 ram:ilbm ram:temp2
			ram:ilbm2raw -cf80,268,32,2 ram:ilbm ram:temp3
			ram:ilbm2raw -cf128,268,96,2 ram:ilbm ram:temp4
			ram:ilbm2raw -cf224,268,32,2 ram:ilbm ram:temp5
			ram:ilbm2raw -cf48,26,32,11 ram:ilbm ram:temp6
			ram:ilbm2raw -cf80,26,32,11 ram:ilbm ram:temp7
			ram:ilbm2raw -cf112,26,32,11 ram:ilbm ram:temp8
			join ram:output.bin ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 ram:temp7 ram:temp8 as ram:temp_output
			delete ram:output.bin
			rename ram:temp_output as ram:output.bin
			ram:ilbm2raw -cf144,26,32,11 ram:ilbm ram:temp1
			ram:ilbm2raw -cf176,26,32,11 ram:ilbm ram:temp2
			ram:ilbm2raw -cf208,26,32,11 ram:ilbm ram:temp3
			ram:ilbm2raw -cf240,26,32,11 ram:ilbm ram:temp4
			ram:ilbm2raw -cf32,78,16,14 ram:ilbm ram:temp5
			ram:ilbm2raw -cf64,76,48,16 ram:ilbm ram:temp6
			ram:ilbm2raw -cf112,76,48,16 ram:ilbm ram:temp7
			ram:ilbm2raw -cf160,76,32,16 ram:ilbm ram:temp8
			join ram:output.bin ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 ram:temp7 ram:temp8 as ram:temp_output
			delete ram:output.bin
			rename ram:temp_output as ram:output.bin
			ram:ilbm2raw -cf192,76,48,16 ram:ilbm ram:temp1
			ram:ilbm2raw -cf240,76,48,16 ram:ilbm ram:temp2
			ram:ilbm2raw -cf288,78,16,14 ram:ilbm ram:temp3
			ram:ilbm2raw -cf48,151,32,23 ram:ilbm ram:temp4
			ram:ilbm2raw -cf80,150,64,24 ram:ilbm ram:temp5
			ram:ilbm2raw -cf144,150,48,24 ram:ilbm ram:temp6
			ram:ilbm2raw -cf192,150,64,24 ram:ilbm ram:temp7
			ram:ilbm2raw -cf256,151,32,23 ram:ilbm ram:temp8
			join ram:output.bin ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 ram:temp7 ram:temp8 as ram:temp_output
			delete ram:output.bin
			rename ram:temp_output as ram:output.bin
			ram:ilbm2raw -cf80,227,48,40 ram:ilbm ram:temp1
			ram:ilbm2raw -cf128,227,80,40 ram:ilbm ram:temp2
			ram:ilbm2raw -cf208,227,48,40 ram:ilbm ram:temp3
			ram:ilbm2raw -cf80,308,32,42 ram:ilbm ram:temp4
			ram:ilbm2raw -cf80,360,144,42 ram:ilbm ram:temp5
			ram:ilbm2raw -cf208,308,32,42 ram:ilbm ram:temp6
			join ram:output.bin ram:temp1 ram:temp2 ram:temp3 ram:temp4 ram:temp5 ram:temp6 as ram:temp_output
			delete ram:output.bin
			rename ram:temp_output as ram:output.bin
			genam light1.s
			strip /raw/light1.bin 32 4

